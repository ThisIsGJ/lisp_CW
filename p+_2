(defun p+(poly1 poly2)
	(format t "~%poly1 ~A" poly1)
	(format t "~%poly2 ~A" poly2)
;;(-2x^3 + 3y +z + 5 ) ’((-2 3 x) (3 1 y) (1 1 z) (5 0 0)）	
	(dolist (x poly1)
		(dolist (y poly2)
			(if (equal (cdr x) (cdr y))
				(print x)
				(print nil)
				)
			)
		)

)

(defun test()
	(p+ '((1 2 x) (2 1 y) (3 1 z)) '((2 1 x) (2 1 y) (2 1 z)))
	)